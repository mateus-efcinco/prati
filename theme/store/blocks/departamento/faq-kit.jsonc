   // 01
  "menu-item#texto-1": {
    "title": "Pergunta 1",
    "props": {
      "type": "custom",
      "text": "Comida Boa e que faz Bem no seu Dia a Dia!"
    },
    "blocks": ["vtex.menu@2.x:submenu.accordion#texto-1"]
  },
  "vtex.menu@2.x:submenu.accordion#texto-1": {
    "title": "Responder",
